{
  "compilerOptions": {
      "lib": [
          "es6"
      ],
      "module": "commonjs",
      "target": "es6",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "noImplicitAny": false,
      "noEmitOnError": true,
      "sourceMap": true,
      "removeComments": true,
      "outDir": "dist"
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
}